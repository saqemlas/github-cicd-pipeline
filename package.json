{
  "name": "@cicd-pipeline/cicd-pipeline",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf node_modules .serverless .esbuild coverage",
    "ci": "yarn install --frozen-lockfile",
    "ci:audit": "yarn run improved-yarn-audit --min-severity moderate --exclude GHSA-93q8-gq69-wqmw",
    "ci:audit:fix": "yarn-audit-fix",
    "checktsc": "tsc --noEmit",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix",
    "test:unit": "jest ./tests/unit --setupFiles ./tests/unit/setupEnv.ts",
    "test:integration": "bash tests/integration/run-test.sh",
    "test:system": "bash tests/system/run-test.sh",
    "invoke": "f() { serverless invoke local --function $1 --path tests/data/$2.json; }; f",
    "deploy": "serverless deploy",
    "remove": "serverless remove"
  },
  "dependencies": {
    "aws-lambda": "1.0.6",
    "lodash-es": "4.17.21",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.38.0",
    "@aws-sdk/client-s3": "3.38.0",
    "@aws-sdk/client-ssm": "3.38.0",
    "@aws-sdk/types": "3.38.0",
    "@aws-sdk/signature-v4-crt": "3.38.0",
    "@types/aws-lambda": "8.10.84",
    "@types/jest": "27.0.2",
    "@types/lodash-es": "4.17.4",
    "@types/node": "16.11.5",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "axios": "0.24.0",
    "aws-sdk-client-mock": "0.5.5",
    "eslint": "8.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-json-format": "2.0.1",
    "improved-yarn-audit": "3.0.0",
    "inquirer": "8.2.0",
    "jest": "27.3.1",
    "rimraf": "3.0.2",
    "serverless": "2.64.1",
    "serverless-esbuild": "1.21.1",
    "serverless-iam-roles-per-function": "3.2.0",
    "serverless-s3-cleaner": "1.0.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "yarn-audit-fix": "7.1.3"
  },
  "resolutions": {
    "uuid": ">=7.0.0"
  }
}
